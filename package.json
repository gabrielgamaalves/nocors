{
  "name": "@gabrielgamaalves/nocors",
  "version": "1.1.0",
  "description": "Advanced HTTP client for Node.js with header control, retries, and client identity profiles.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "dist",
    "data",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && npx ts-node ./.dev/index.ts plugin all",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "dev:run": "ts-node ./src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nocors",
    "cors",
    "http-client",
    "fetch",
    "proxy",
    "nodejs",
    "advanced-http",
    "client-identity",
    "headers",
    "api-integration"
  ],
  "author": "Gabriel Gama <gabrielgamaalves1@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielgamaalves/nocors.git"
  },
  "homepage": "https://github.com/gabrielgamaalves/nocors#readme",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^24.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
